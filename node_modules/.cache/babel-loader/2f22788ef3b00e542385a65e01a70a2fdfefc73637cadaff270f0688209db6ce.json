 }\n\n        span.on {\n            display: none !important;\n        }\n    }\n\n    span.off {\n        @extend %switch;\n        left: 5px;\n        display: none;\n    }\n\n    span.on {\n        @extend %switch;\n        right: 5px !important;\n    }\n\n    button.k-success {\n        @include textStyles('Maven Pro', 700, 14px, 116%);\n        text-align: center;\n        color: #E6F7EE;\n        background: #05B050;\n        border-radius: 1.29167px;\n    }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}