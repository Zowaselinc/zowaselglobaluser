{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AuthSharedLayout from \"@/layouts/shared/AuthSharedLayout.vue\";\nimport states from \"@/data/states\";\nexport default {\n  name: 'CompanyDetails',\n  components: {\n    AuthSharedLayout\n  },\n  data() {\n    return {\n      type: this.$route.params.type,\n      states: states\n    };\n  },\n  methods: {\n    submitForm(e) {\n      var form = new FormData(document.querySelector('#register-form'));\n      var registerData = {\n        has_company: true,\n        company_name: form.get('company-name'),\n        company_address: form.get('company-address'),\n        company_email: form.get('company-email'),\n        company_state: form.get('state'),\n        rc_number: form.get('rc-number'),\n        company_phone: form.get('company-phone')\n      };\n\n      //Check for subtype\n      var subtype = form.get('subtype');\n      if (subtype) {\n        if (this.type == \"agent\") {\n          registerData['agent_type'] = subtype;\n        }\n        if (this.type == \"partner\") {\n          registerData['partnership_type'] = subtype;\n        }\n      }\n      var previousData = JSON.parse(window.localStorage.getItem('registerData'));\n      window.localStorage.setItem('registerData', JSON.stringify({\n        ...previousData,\n        ...registerData\n      }));\n      this.$router.push(`/${this.type}/verifyaccount`);\n    }\n  }\n};","map":{"version":3,"mappings":";AA4FA,OAAOA,gBAAe,MAAO,uCAAuC;AACpE,OAAOC,MAAK,MAAO,eAAe;AAC9B,eAAe;EACXC,IAAI,EAAC,gBAAgB;EACrBC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAI,GAAE;IACF,OAAO;MACHC,IAAG,EAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,IAAI;MAC9BJ,MAAK,EAAIA;IACb,CAAC;EACL,CAAC;EACDO,OAAO,EAAC;IACJC,UAAU,CAACC,CAAC,EAAC;MAET,IAAIC,IAAG,GAAI,IAAIC,QAAQ,CAACC,QAAQ,CAACC,aAAa,CAAC,gBAAgB,CAAC,CAAC;MAEjE,IAAIC,YAAW,GAAI;QACfC,WAAU,EAAI,IAAI;QAClBC,YAAW,EAAIN,IAAI,CAACO,GAAG,CAAC,cAAc,CAAC;QACvCC,eAAc,EAAIR,IAAI,CAACO,GAAG,CAAC,iBAAiB,CAAC;QAC7CE,aAAY,EAAIT,IAAI,CAACO,GAAG,C